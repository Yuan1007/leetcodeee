public int findSpecialInteger(int[] arr) {
        int[] newArr = new int[100001];
        int max = 0;
        Arrays.fill(newArr, 0);
        for(int i=0; i<arr.length; i++){
            newArr[arr[i]]++;
            max = Math.max(max, newArr[arr[i]]);
        }

        for(int i=0; i<newArr.length; i++){
            if(newArr[i] == max) return i;
        }

        return 0;
    }
